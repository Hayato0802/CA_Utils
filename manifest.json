{
  "manifest_version": 3,
  "name": "Co-Assign Utils 2",
  "description": "Co-Assign Utils (auto update)",
  "version": "2.0.1",
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "alarms",
    "tabs"
  ],
  "host_permissions": [
    "https://p.ieyasu.co/*",
    "https://*.ieyasu.co/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.co-assign.com/worksheet/*"
      ],
      "js": [
        "config.js",
        "utils.js",
        "content-script-coassign.js"
      ]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/co_assign_utils_icon_16.png",
      "48": "images/co_assign_utils_icon_48.png",
      "128": "images/co_assign_utils_icon_128.png"
    }
  }
}